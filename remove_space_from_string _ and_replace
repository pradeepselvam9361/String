import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter string:");
        String str = sc.nextLine();
        String str1 = "";
        for(int i = 0;i<str.length();i++){
            char ch = str.charAt(i);
            if(ch == ' '){
                ch = '-';
            }
            str1+=ch;
        }
        System.out.print(str1);
    }
}